{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@main": ["index.ts"],
			"@actions/*": ["actions/*"],
			"@utils": ["utils/index.ts"],
			"@utils/*": ["utils/*"],
			"@context/*": ["context/*"],
			"@api/*": ["api/*"],
			"@db/*": ["db/*"],
			"@client/*": ["client/*"],
			"@config": ["config/index.ts"],
			"@config/*": ["config/*"]
		},
		"jsx": "react-jsx",
		"esModuleInterop": true
	},
	"include": ["src/**/*"]
}
