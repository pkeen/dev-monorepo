{
	"name": "@pete_keen/north",
	"version": "0.0.1",
	"main": "dist/cjs/index.js",
	"type": "module",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"types": "./dist/types/index.d.ts"
		},
		"./handlers": {
			"import": "./dist/esm/handlers/index.js",
			"require": "./dist/cjs/handlers/index.js",
			"types": "./dist/types/handlers/index.d.ts"
		},
		"./utils": {
			"import": "./dist/esm/utils/index.js",
			"require": "./dist/cjs/utils/index.js",
			"types": "./dist/types/utils/index.d.ts"
		},
		"./components": {
			"import": "./dist/esm/components/index.js",
			"require": "./dist/cjs/components/index.js",
			"types": "./dist/types/components/index.d.ts"
		},
		"./test": {
			"import": "./dist/esm/test/index.js",
			"require": "./dist/cjs/test/index.js",
			"types": "./dist/types/test/index.d.ts"
		}
	},
	"scripts": {
		"build": "rimraf dist && npm run build:types && npm run build:js && npm run build:copy",
		"build:types": "tsc --emitDeclarationOnly --outDir dist/types",
		"build:js": "rollup -c",
		"build:copy": "copyfiles -u 1 \"dist/types/**/*.d.ts\" dist/esm && copyfiles -u 1 \"dist/types/**/*.d.ts\" dist/cjs",
		"dev": "rollup -c -w"
	},
	"dependencies": {
		"axios": "^1.7.8",
		"bcryptjs": "^2.4.3",
		"cookie": "^1.0.2",
		"dotenv": "^16.4.5",
		"jose": "^5.9.6",
		"tslib": "^2.8.1",
		"vite": "^6.0.1",
		"vite-plugin-dts": "^4.3.0",
		"zod": "^3.23.8"
	},
	"peerDependencies": {
		"@neondatabase/serverless": "^0.10.3",
		"drizzle-orm": "^0.36.3",
		"next": "15.0.3",
		"react": "19.0.0-rc-66855b96-20241106",
		"react-dom": "19.0.0-rc-66855b96-20241106"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.2.0",
		"@neondatabase/serverless": "^0.10.3",
		"@rollup/plugin-alias": "^5.1.1",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.3.0",
		"@rollup/plugin-typescript": "^8.5.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^20.17.6",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18",
		"@vitejs/plugin-react": "^4.3.4",
		"copyfiles": "^2.4.1",
		"drizzle-kit": "^0.28.1",
		"drizzle-orm": "^0.36.3",
		"eslint": "^8",
		"eslint-import-resolver-alias": "^1.1.2",
		"rimraf": "^6.0.1",
		"rollup": "^3.29.5",
		"rollup-plugin-copy": "^3.5.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-preserve-directives": "^0.4.0",
		"rollup-plugin-preserve-use-client": "^3.0.1",
		"typescript": "^5.7.2",
		"next": "15.0.3",
		"react": "19.0.0-rc-66855b96-20241106",
		"react-dom": "19.0.0-rc-66855b96-20241106"
	}
}
